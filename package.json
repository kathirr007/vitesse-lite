  },
  "dependencies": {
    "@vueuse/core": "^10.9.0",
    "@vueuse/head": "^2.0.0",
    "vue": "^3.4.27",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.1",
    "@iconify/vue": "^4.1.2",
    "@types/node": "^20.12.12",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue-macros/volar": "^0.19.1",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.3.0",
    "eslint-plugin-format": "^0.1.1",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "pnpm": "^9.1.1",
    "postcss": "^8.4.39",
    "simple-git-hooks": "^2.11.1",
    "tailwindcss": "^3.4.4",
    "taze": "^0.13.8",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.0",
    "unplugin-vue-macros": "^2.9.2",
    "unplugin-vue-router": "^0.8.6",
    "vite": "^5.2.11",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.19"
